#include <math.h> 
#include <stdio.h> 
#include <stdlib.h>
#include <float.h>

/**
 * @brief Функция расчета формулы
 * @param x значение параметра
 * @return Значение вычисления y  
*/
float function(float x);

/**
 * @brief Функция проверки ввода на число 
 * @param value значение вводимого параметра 
 * @return Результат проверки   
*/
double fun_scan(double value);

/**
 * @brief Функция проверки ввода концов отрезка 
 * @param x1, x2 значения вводимыъх параметров 
 * @return Результат проверки   
*/
double check_function(float x1, float x2);

/**
 * @brief Точка входа в программу  
 * @return Вывод 0, если программа работает неверно. Иначе вывод 1  
*/
int main () 
{ 
    float x, y, w, x1 = fun_scan(), x2 = fun_scan(), x3 = fun_scan(); 
    w=chec_function(x1, x2);
    for (x=x1; x<x2+0.2; x+=x3)
    {
        y=function(x);
        printf ("y=%f\n x=%f\n", y, x); 
    }
    return 0;
}

float function(float x)
{
    return (3*pow(log(x), 2) + 6*log(x)-5);
}

double check_function(float x1, float x2)
{
    if (x1 - x2 > -DBL_EPSILON)
    {
        puts("Wrong numbers");
        abort();
    }
}
double fun_scan(double value) 
{
  double value;
  int result = scanf("%lf", &value);
    if ((result != 1)||(result<0))
    {
      puts("error io");
      abort();
    } 
    return value;
}
