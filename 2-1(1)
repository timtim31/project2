#include <stdio.h>
#include <stdlib.h>

/**
 * @brief Функция проверки ввода на число 
 * @param value значение вводимого параметра 
 * @return Результат проверки   
*/
double fun_scan();

/**
 * @brief Функция вывода чисел в порядке убывания 
 * @param a, b, c значения вводимых параметров
 * @return Результат функции    
*/
int max(int a, int b, int c, int maximum, int average, int minimum);

/**
 * @brief Функция вывода чисел в порядке возрастания  
 * @param a, b, c значения вводимых параметров
 * @return Результат функции    
*/
int min(int a, int b, int c, int maximum, int average, int minimum);

/**
 * @brief Вводимое значение в программу 
 * @return Вывод 0, если программа неверна. Иначе вывод 1
*/
int main();

int main()
{
    int a, b, c, z, q;
    int maximum = 0;
    int average = 0;
    int minimum = 0;
    a = fun_scan();
    b = fun_scan();
    c = fun_scan();
    z = max(a,b,c,maximum, average, minimum);
    q = min(a,b,c,maximum, average, minimum);
    
    printf("%d\n", z);
    printf("%d\n", q);
    return 0;
}

int max(int a, int b, int c, int maximum, int average, int minimum)
{
    if ((a>b)&&(a>c))
    {
        maximum=a;
        if (b<c)
        {
            minimum=b;
            average=c;
            printf(" %d\n%d\n%d\n", maximum, average, minimum);
        }
        else
        {
            minimum=c;
            average=b;
            printf(" %d\n%d\n%d\n", maximum, average, minimum);
        }
    }
    if ((b>a)&&(b>c))
    {
        maximum=b;
        if (a<c)
        {
            minimum=a;
            average=c;
            printf(" %d\n%d\n%d\n", maximum, average, minimum);
        }
        else
        {
            minimum=c;
            average=a;
            printf(" %d\n%d\n%d\n", maximum, average, minimum);
        }
    }
    if ((c>a)&&(c>b))
    {
        maximum=c;
        if (a<b)
        {
            minimum=a;
            average=b;
            printf(" %d\n%d\n%d\n", maximum, average, minimum);
        }
        else
        {
            minimum=b;
            average=a;
            printf(" %d\n%d\n%d\n", maximum, average, minimum);
        }
    }
}

int min(int a, int b, int c, int maximum, int average, int minimum)
{
    if ((a<b)&&(a<c))
    {
        minimum=a;
        if (b<c)
        {
            maximum=c;
            average=b;
            printf(" %d\n%d\n%d\n", minimum, average, maximum);
        }
        else
        {
            maximum=b;
            average=c;
            printf(" %d\n%d\n%d\n", minimum, average, maximum);
        }
    }
    if ((b<a)&&(b<c))
    {
        minimum=b;
        if (a<c)
        {
            maximum=c;
            average=a;
            printf(" %d\n%d\n%d\n", minimum, average, maximum);
        }
        else
        {
            maximum=a;
            average=c;
            printf(" %d\n%d\n%d\n", minimum, average, maximum);
        }
    }
    if ((c<a)&&(c<b))
    {
        minimum=c;
        if (a<b)
        {
            maximum=b;
            average=a;
            printf(" %d\n%d\n%d\n", minimum, average, maximum);
        }
        else
        {
            maximum=a;
            average=b;
            printf(" %d\n%d\n%d\n", minimum, average, maximum);
        }
    }
}


double fun_scan() 
{
  double value;
  int result = scanf("%lf", &value);
    if (result != 1)
    {
      printf("error io");
      abort();
    } 
    return value;
}
