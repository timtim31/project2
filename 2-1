#include <stdio.h>
#include <stdlib.h>
enum Numbers {
    first,second,third
};
int a=first;
int b=second;
int c=third;

/**
 * @brief Функция проверки ввода на число 
 * @param value значение вводимого параметра 
 * @return Результат проверки   
*/
double fun_scan();

/**
 * @brief Вводимое значение в программу 
 * @return Вывод 0, если программа неверна. Иначе вывод 1
*/
int main();


int main()
{
    int a, b, c, z;
    a = fun_scan();
    b = fun_scan();
    c = fun_scan();
        if (a > c) {
            z = a; a = c; c = z;
        }
        if (a > b) {
            z = a; a = b; b = z;
        }
        if (b > c) {
            z = b; b = c; c = z;
        }
 
        printf(" %d\n%d\n%d\n", a, b, c);
        if (a < c) {
            z = a; a = c; c = z;
        }
        if (a < b) {
            z = a; a = b; b = z;
        }
        if (b < c) {
            z = b; b = c; c = z;
        }
        printf(" %d\n%d\n%d\n", a, b, c);
    return 0;
}

double fun_scan() {
  double value;
  int result = scanf("%lf", &value);
    if (result != 1)
    {
      printf_s("error io");
      abort();
    } 
    return value;
}
